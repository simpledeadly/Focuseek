{
  "name": "vue-from-scratch",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "eslint-check": "eslint --ext .vue,.js,.ts .",
    "eslint-fix": "eslint --ext .vue,.js,.ts . --fix",
    "prettier-check": "prettier . \"!**/*.{vue,js,ts}\" --check",
    "prettier-fix": "prettier . \"!**/*.{vue,js,ts}\" --write",
    "stylelint-fix": "stylelint \"**/*.{vue,scss}\" --fix"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "sass": "^1.77.8",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-stylelint": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
